{
  "name": "league-dao-nomoPlayersDropMechanicMock",
  "version": "1.0.0",
  "description": "Repo for ERC721 distribution mechanism",
  "main": "hardhat.config.js",
  "scripts": {
    "coverage": "env-cmd --silent -f .env.dev npx hardhat coverage",
    "test": "env-cmd --silent -f .env.dev npx hardhat test",
    "contracts:compile": "env-cmd --silent -f env.dev npx hardhat compile",
    "contracts:migrate:dev:rinkeby": "env-cmd --silent -f .env.dev npx hardhat deploy-nomoDropMechanic --network rinkeby",
    "contracts:verify:dev:rinkeby": "env-cmd --silent -f .env.dev npx hardhat verify-nomoDropMechanic --network rinkeby",
    "contracts:erc721Mock:rinkeby": "env-cmd --silent -f .env.dev npx hardhat deploy-verify-erc721 --network rinkeby",
    "contracts:setApproval:rinkeby": "env-cmd --silent -f .env.dev npx hardhat erc721-setApprovalForAll --network rinkeby",
    "contracts:random:rinkeby": "env-cmd --silent -f .env.dev npx hardhat airdrop-requestRandomValue --network rinkeby",
    "contracts:airdrop:rinkeby": "env-cmd --silent -f .env.dev npx hardhat airdrop-execute --network rinkeby",
    "contracts:migrate:mumbai": "env-cmd --silent -f .env.dev npx hardhat deploy-nomoPlayersDropMechanic --network mumbai",
    "contracts:verify:mumbai": "env-cmd --silent -f .env.dev npx hardhat verify-nomoPlayersDropMechanic --network mumbai",
    "contracts:erc721Mock:mumbai": "env-cmd --silent -f .env.dev npx hardhat deploy-verify-erc721 --network mumbai",
    "contracts:setApproval:mumbai": "env-cmd --silent -f .env.dev npx hardhat erc721-setApprovalForAll --network mumbai",
    "contracts:random:mumbai": "env-cmd --silent -f .env.dev npx hardhat airdrop-requestRandomValue --network mumbai",
    "contracts:airdrop:mumbai": "env-cmd --silent -f .env.dev npx hardhat airdrop-execute --network mumbai",
    "contracts:migrate:prod": "env-cmd --silent -f .env.prod npx hardhat deploy-nomoDropMechanic --network mainnet",
    "contracts:verify:prod": "env-cmd --silent -f .env.prod npx hardhat verify-nomoDropMechanic --network mainnet",
    "contracts:setApproval:prod": "env-cmd --silent -f .env.prod npx hardhat erc721-setApprovalForAll --network mainnet",
    "contracts:random:prod": "env-cmd --silent -f .env.prod npx hardhat airdrop-requestRandomValue --network mainnet",
    "contracts:airdrop:prod": "env-cmd --silent -f .env.prod npx hardhat airdrop-nomoDropMechanic --network mainnet"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.6",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.3.2",
    "@typechain/ethers-v5": "^7.1.2",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.2",
    "chai": "^4.3.4",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.7",
    "hardhat": "^2.6.4",
    "hardhat-abi-exporter": "^2.3.0",
    "hardhat-contract-sizer": "^2.1.1",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-typechain": "^0.3.5",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-beta.18",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.17",
    "ts-node": "^10.2.1",
    "typechain": "^5.1.2",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@appliedblockchain/chainlink-contracts": "0.0.4",
    "@appliedblockchain/chainlink-plugins-fund-link": "0.0.3",
    "@chainlink/contracts": "^0.2.2",
    "@chainlink/token": "^1.1.0",
    "dotenv": "^10.0.0"
  }
}
